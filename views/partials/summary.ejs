<section class="summary">

    <h2>Smartzones</h2>
    <ul class="smartzones">
        <% smartzones.smartzones.forEach(smartzone=> { %>
            <li>
                <div class="container-img">
                    <img src="<%= smartzone.image[0].url %>" alt="">
                </div>
                <div class="container-li">
                    <% if (smartzone.reservation==null) { %>
                        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20" fill="none">
                            <path
                                d="M19 10C19 11.1819 18.7672 12.3522 18.3149 13.4442C17.8626 14.5361 17.1997 15.5282 16.364 16.364C15.5282 17.1997 14.5361 17.8626 13.4442 18.3149C12.3522 18.7672 11.1819 19 10 19C8.8181 19 7.64778 18.7672 6.55585 18.3149C5.46392 17.8626 4.47177 17.1997 3.63604 16.364C2.80031 15.5282 2.13738 14.5361 1.68508 13.4442C1.23279 12.3522 1 11.1819 1 10C1 7.61305 1.94821 5.32387 3.63604 3.63604C5.32387 1.94821 7.61305 1 10 1C12.3869 1 14.6761 1.94821 16.364 3.63604C18.0518 5.32387 19 7.61305 19 10Z"
                                stroke="#524DD0" stroke-width="2" />
                            <path d="M6 11L9 14L14 7" stroke="#524DD0" stroke-width="2" stroke-linecap="round"
                                stroke-linejoin="round" />
                        </svg>
                        Now available
                        <% } else { %>
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20"
                                fill="none">
                                <path
                                    d="M19 10C19 11.1819 18.7672 12.3522 18.3149 13.4442C17.8626 14.5361 17.1997 15.5282 16.364 16.364C15.5282 17.1997 14.5361 17.8626 13.4442 18.3149C12.3522 18.7672 11.1819 19 10 19C8.8181 19 7.64778 18.7672 6.55585 18.3149C5.46392 17.8626 4.47177 17.1997 3.63604 16.364C2.80031 15.5282 2.13738 14.5361 1.68508 13.4442C1.23279 12.3522 1 11.1819 1 10C1 7.61305 1.94821 5.32387 3.63604 3.63604C5.32387 1.94821 7.61305 1 10 1C12.3869 1 14.6761 1.94821 16.364 3.63604C18.0518 5.32387 19 7.61305 19 10Z"
                                    stroke="#524DD0" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                                <path d="M13 7L10 10M7 13L10 10M10 10L13 13M10 10L7 7" stroke="#524DD0" stroke-width="2"
                                    stroke-linecap="round" stroke-linejoin="round" />
                            </svg>
                            Occupied
                            <% } %>

                                <h3>SZ <%= smartzone.name %>
                                </h3>
                                <p>
                                    <%= smartzone.address %>, <%= smartzone.town %>
                                </p>
                                <p><span>Price:</span> â‚¬1,00 / 15 min</p>
                                <% if (smartzone.reservation==null) { %>
                                    <button class="book-now" style="--reserved-bg: #f6f6fc; --reserved-c: #6a6b7b;">Book
                                        now</button>
                                    <% } else { %>
                                        <a href="" class="book-now">Book now</a>
                                        <% } %>
                                            <a href="http://localhost:8000/book?id=<%= smartzone.id %>">Book later</a>
                                            <button class="info-button" title="meer informatie">
                                                <svg class="info-svg" xmlns="http://www.w3.org/2000/svg" width="21"
                                                    height="21" viewBox="0 0 21 21" fill="none">
                                                    <path
                                                        d="M8.91667 8.60671L8.97437 8.57616C9.15484 8.47829 9.35737 8.43861 9.55797 8.46181C9.75858 8.48502 9.94885 8.57014 10.1063 8.7071C10.2637 8.84406 10.3816 9.02711 10.4462 9.23459C10.5107 9.44207 10.5191 9.66528 10.4704 9.87779L9.474 14.2104C9.42494 14.4231 9.4331 14.6465 9.49749 14.8542C9.56189 15.062 9.67982 15.2453 9.8373 15.3825C9.99479 15.5196 10.1852 15.6049 10.386 15.6282C10.5868 15.6514 10.7895 15.6116 10.9701 15.5136L11.0278 15.4815M9.97222 5.60672H9.98348V5.61894H9.97222V5.60672ZM20 10.5C20 11.7476 19.7543 12.9829 19.2769 14.1355C18.7994 15.2881 18.0997 16.3354 17.2175 17.2175C16.3354 18.0997 15.2881 18.7994 14.1355 19.2769C12.9829 19.7543 11.7476 20 10.5 20C9.25244 20 8.0171 19.7543 6.86451 19.2769C5.71191 18.7994 4.66464 18.0997 3.78249 17.2175C2.90033 16.3354 2.20056 15.2881 1.72314 14.1355C1.24572 12.9829 1 11.7476 1 10.5C1 7.98045 2.00089 5.56407 3.78249 3.78249C5.56408 2.0009 7.98044 1 10.5 1C13.0196 1 15.4359 2.0009 17.2175 3.78249C18.9991 5.56407 20 7.98045 20 10.5Z"
                                                        stroke="black" stroke-width="2" stroke-linecap="round"
                                                        stroke-linejoin="round" />
                                                </svg>
                                            </button>
                </div>
            </li>

            <article class="detail">
                <img src="<%= smartzone.image[0].url %>" alt="">
                <header>
                    <svg width="24" height="24" xmlns="http://www.w3.org/2000/svg" fill-rule="evenodd"
                        clip-rule="evenodd">
                        <path
                            d="M2.117 12l7.527 6.235-.644.765-9-7.521 9-7.479.645.764-7.529 6.236h21.884v1h-21.883z" />
                    </svg>
                    <%= smartzone.address %>
                </header>
                <dl>
                    <dt>Name</dt>
                    <dd>
                        <%= smartzone.address %>
                </dl>
                <dt>Description</dt>
                <dd>
                    <%= smartzone.description %>
                        </dl>
                <dt>Size</dt>
                <dd>
                    <%= smartzone.size %>
                        </dl>
                <dt>Utilization</dt>
                <dd>
                    <%= smartzone.utilization %>
                        </dl>
                        </dl>
                        <a href="https://www.google.nl/maps/place/<%= smartzone.address %>" target="blank">Bekijken in
                            Google maps</a>
            </article>
            <% }) %>
    </ul>

</section>